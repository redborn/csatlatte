<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="isttt">

	<resultMap type="IstttVo" id="IstttVoMap">
		<result column="isttt_seq" property="istttSequence"/>
		<result column="isttt_name" property="istttName"/>
	</resultMap>
	
	<select id="selectList" resultMap="IstttVoMap">
		SELECT isttt_seq,isttt_name
		  FROM csat_isttt
	</select>
</mapper>
